{
  "compilerOptions": {
    "target": "ES2021",
    "module": "ES2020",
    "rootDir": "./src",
    "baseUrl": "./src",
    "paths": {
      "/*": ["*"]
    },
    "sourceMap": false,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
